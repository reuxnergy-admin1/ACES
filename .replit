modules = ["nodejs-20", "web", "python-3.11"]
[agent]
expertMode = true
integrations = ["github:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["imagemagick"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Development Server"

[[workflows.workflow]]
name = "Next.js Development Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pnpm dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33207
externalPort = 3001

[[ports]]
localPort = 34769
externalPort = 3002

[[ports]]
localPort = 36289
externalPort = 3000

[[ports]]
localPort = 37071
externalPort = 4200

[[ports]]
localPort = 44119
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["pnpm", "start"]
build = ["pnpm", "build"]
