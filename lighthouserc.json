{
  "ci": {
    "collect": {
      "staticDistDir": "out",
      "url": ["/", "/products/", "/services/", "/blog/"],
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", {"minScore": 0.85}],
        "categories:accessibility": ["warn", {"minScore": 0.9}]
      }
    }
  }
}
