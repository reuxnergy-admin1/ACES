{
  "name": "aces-aerodynamics-site",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "lh:ci": "npx @lhci/cli collect --config=./lighthouserc.json && npx @lhci/cli assert --config=./lighthouserc.json",
    "test:e2e": "playwright test",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:reference": "playwright test tests/external-audit.spec.ts",
    "test:e2e:install": "playwright install --with-deps",
    "guard:bg": "node ./scripts/bg-guard.mjs verify",
    "guard:bg:update": "node ./scripts/bg-guard.mjs update"
  },
  "dependencies": {
    "@react-three/fiber": "^9.3.0",
    "clsx": "2.1.1",
    "next": "15.5.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "4.1.12",
    "@tailwindcss/typography": "0.5.16",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.179.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.2",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.12",
    "typescript": "5.9.2"
  }
}